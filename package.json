{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.8.0",
  "engines": {
    "node": "22.14.0",
    "pnpm": "10.8.0"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kriegcloud/beephole.git"
  },
  "scripts": {
    "clean": "turbo clean",
    "codegen": "turbo codegen",
    "build": "turbo build",
    "check": "turbo check",
    "check:all": "pnpm lint && pnpm check && pnpm test",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-export-namespace-from": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.25.9",
    "@effect/build-utils": "0.7.9",
    "@effect/language-service": "0.5.1",
    "@effect/vitest": "0.20.6",
    "@types/dotenv": "^8.2.3",
    "@types/node": "^22.8.5",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "effect": "3.14.6",
    "glob": "^11.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "read-package-up": "^11.0.0",
    "tsx": "^4.19.2",
    "turbo": "^2.5.0",
    "typescript": "latest",
    "vitest": "^2.1.4"
  },
  "pnpm": {
    "overrides": {
      "vitest": "^2.0.5"
    },
    "patchedDependencies": {
      "babel-plugin-annotate-pure-calls@0.4.0": "patches/babel-plugin-annotate-pure-calls@0.4.0.patch"
    },
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@parcel/watcher",
      "esbuild",
      "msgpackr-extract",
      "protobufjs",
      "sharp"
    ]
  }
}
