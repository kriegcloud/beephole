{
  "compilerOptions": {
    "strict": true,
    "moduleDetection": "force",
    "composite": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "types": [],
    "isolatedModules": true,
    "sourceMap": true,
    "declarationMap": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": false,
    "exactOptionalPropertyTypes": false,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "target": "ES2022",
    "incremental": true,
    "removeComments": false,
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ],
    "paths": {
      "@beep/domain": ["./packages/domain/src/index.js"],
      "@beep/domain/*": ["./packages/domain/src/*.js"],
      "@beep/domain/test/*": ["./packages/domain/test/*.js"],
      "@beep/server": ["./packages/server/src/index.js"],
      "@beep/server/*": ["./packages/server/src/*.js"],
      "@beep/server/test/*": ["./packages/server/test/*.js"],
      "@beep/database": ["./packages/database/src/index.js"],
      "@beep/database/*": ["./packages/database/src/*.js"],
      "@beep/database/test/*": ["./packages/database/test/*.js"],
      "@beep/kernel": ["./packages/kernel/src/index.js"],
      "@beep/kernel/*": ["./packages/kernel/src/*.js"],
      "@beep/kernel/test/*": ["./packages/kernel/test/*.js"]
    }
  }
}
